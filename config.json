//android

{
    "type": "success",
    "data": {
        "value": "AVAILABLE",
        "info":{
            {
                "webview": {
                    "settings": {
                        "javaScriptEnabled": true,
                        "databaseEnabled": true,
                        "domStorageEnabled": true,
                        "allowFileAccess": true,
                        "geolocationEnabled": true,
                        "renderPriority": "HIGH",
                        "mixedContentMode": "MIXED_CONTENT_ALWAYS_ALLOW",
                        "javaScriptCanOpenWindowsAutomatically": true,
                        "mediaPlaybackRequiresUserGesture": false,
                        "cacheMode": "LOAD_DEFAULT",
                        "layerType": "HARDWARE",
                        "userAgent": {
                            "append": "_Partner_Android"
                        }
                    },
                    "urlHandling": {
                        "allowDirectLoadingPatterns": [
                            ".pdf",
                            "/statements/",
                            "/download_statements"
                        ],
                        "redirectPatterns": [
                            {
                                "pattern": "/redirect?status=",
                                "action": "EXIT_WITH_STATUS"
                            },
                            {
                                "pattern": "session-expired",
                                "action": "LOGOUT"
                            }
                        ],
                        "externalAppPatterns": [
                            {
                                "pattern": "api.whatsapp.com",
                                "action": "OPEN_WHATSAPP"
                            }
                        ]
                    },
                    "downloads": {
                        "enabled": true,
                        "requireStoragePermission": true,
                        "destination": "DIRECTORY_DOWNLOADS",
                        "autoOpenAfterDownload": true
                    }
                }
            }
        }
    }
}
